{
  "name": "@waveswap/programs",
  "version": "0.1.0",
  "description": "Anchor Solana programs for WaveSwap privacy-preserving DEX aggregator",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "require": "./lib/index.js",
      "types": "./lib/index.d.ts"
    },
    "./idls": "./lib/idls/"
  },
  "files": [
    "lib",
    "README.md"
  ],
  "scripts": {
    "build": "anchor build",
    "build:devnet": "anchor build --provider.cluster devnet",
    "deploy:devnet": "anchor deploy --provider.cluster devnet",
    "deploy:localnet": "anchor deploy --provider.cluster localnet",
    "test": "anchor test",
    "test:skip-build": "anchor test --skip-build",
    "test:devnet": "anchor test --provider.cluster devnet",
    "anchor:build": "anchor build",
    "anchor:test": "anchor test",
    "anchor:deploy": "anchor deploy",
    "lint": "eslint programs --ext .ts",
    "lint:fix": "eslint programs --ext .ts --fix",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf target lib",
    "idl:extract": "anchor idl extract --file lib/idls/waveswap_swap_registry.json",
    "idl:generate": "anchor idl build"
  },
  "dependencies": {
    "@coral-xyz/anchor": "^0.30.1",
    "@solana/web3.js": "^1.95.0",
    "@solana/spl-token": "^0.4.8"
  },
  "devDependencies": {
    "@types/bn.js": "^5.2.0",
    "@types/chai": "^5.2.3",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "chai": "^5.0.0",
    "eslint": "^9.0.0",
    "mocha": "^10.0.0",
    "ts-mocha": "^10.0.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5.6.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "solana",
    "blockchain",
    "anchor",
    "rust",
    "defi",
    "dex",
    "privacy",
    "cryptography",
    "waveswap"
  ],
  "author": "WaveSwap Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/waveswap/waveswap.git",
    "directory": "packages/programs"
  }
}